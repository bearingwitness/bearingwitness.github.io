@misc{theberkeleyforumDrAnthonyFauci2020,
  title = {Dr. {{Anthony Fauci}} Participates in {{COVID}}-19 Fireside Chat, Touches on Systemic Racial Injustice},
  author = {{The Berkeley Forum}},
  year = {2020},
  month = oct,
  abstract = {Dr. Anthony Fauci participated in a fireside chat as part of the University of California Berkley Forum. Asked about systemic racial injustices and COVID, Fauci noted his time working at the outset of the AIDS epidemic. He noted African Americans made up 13\% of the U.S. population, but 44\% of the infections at the time. There is a similar disproportionate rate of infection during the COVID-19 pandemic. Now, Dr. Fauci noted that Black, Latinx, Indigenous and other minority demographics in America have two things going against them: First, a greater likelihood to contract coronavirus by the very nature of jobs prevalent in minority communities. Second, a higher rate of comorbidities, which leads to a higher susceptibility of hospitalization and death. Fauci highlighted how the latter, especially, is due to social determinants of health that many people are born in to. The answer to this, as COVID-19 has shed a bright light on social determinants of health that are essentially killing people, he concluded, is systemic change. Dr. Fauci was also asked about an equitable distribution of any COVID-19 vaccine that is developed. He referenced the CDC Advisory Committee on Immunization Practices, as well as the federal Panel, the National Academies', efforts in coming up with a just framework for distribution.}
}

