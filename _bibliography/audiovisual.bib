@misc{the_intercept_covid-19_nodate,
	title = {Covid-19 {Transforming} {Culture} and {Media} in {The} {United} {States} - {System} {Update} with {Murtaza} {Hussain}},
	url = {https://www.youtube.com/watch?v=9EXCMLMxuIY},
	abstract = {Nesrine Malik, in the Intercept’s podcast “System Update,” identifies a range of national myths that have been laid bare by the COVID-19 Pandemic. Broadly, she characterizes these myths as functioning to assure a nation, and in this case, the United States and Britain, that it is doing better than it actually is, that its exceptionalism has and will continue to prevail in the face of a large health and economic crisis. Malik details that, as COVID-19 disproportionately affected people along racial lines (via high representation as essential labor, and also access or lack of access to health care) it became clear that the mobilization of black and ethnic minorities based in part on identity was necessary. Race is inextricable from class, Malik expands, disproving any claim that identity politics is the “indulgence” of the youth or woke culture. Malik’s final thesis posits that the reason these myths exist is that the system is actually very weak. Myths of exceptionalism serve a purpose and obfuscate the broken or insufficient systems within these two states.},
	urldate = {2020-09-08},
	author = {{The Intercept}}
}

