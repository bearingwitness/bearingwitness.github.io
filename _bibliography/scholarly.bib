@article{blockAfricanAmericanAdherence2020,
  title = {African {{American Adherence}} to {{COVID}}-19 {{Public Health Recommendations}}},
  author = {Block, Ray and Berg, Arthur and Lennon, Robert P and Miller, Erin L and {Nunez-Smith}, Marcella},
  year = {2020},
