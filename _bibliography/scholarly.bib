  file = {/Users/alex/Zotero/storage/A7KB87KZ/Abbas et al. - 2020 - COVID-19 infection and its deadly cytokine storm i.pdf;/Users/alex/Zotero/storage/MJUKKVB8/20009666.2020.html},
  journal = {Journal of Community Hospital Internal Medicine Perspectives; Abington},
  keywords = {COVID19,morbid obesity,mortality,race,risk factors},
