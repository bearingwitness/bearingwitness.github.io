@techreport{richardsonLastingImpactHistoric2020,
  title = {The {{Lasting Impact}} of {{Historic}} '{{Redlining}}' on {{Neighborhood Health}}: {{Higher Prevalence}} of {{Covid}}-19 {{Risk Factors}}},
  author = {Richardson, Jason and Mitchell, Bruce and Meier, Helen and Lynch, Emily and Edlebi, Jad and Nelson, Robert and Madron, Justin and Madron, Justin},
  year = {2020},
  month = sep,
  pages = {50},
  institution = {{National Community Reinvestment Coalition}},
  abstract = {A study conducted by the National Community Reinvestment Coalition found a higher prevalence of COVID-19 risk factors  throughout neighborhoods that were historically ``redlined.'' Redlining, the process of withholding lending and investment from low-income and minority communities in particular neighborhoods, dating from at least the 1930s, correlates today with reduced health, greater poverty, lower life expectancy, and higher incidence of chronic disease. These, the latter two especially, are risk factors for poor outcomes for COVID-19. Historical redlining is related to present-day neighborhood characteristics including minority presence, higher occurrences of poverty, and greater social vulnerability (meaning people ``have a decreased capability to withstand natural and human-created disasters because of their more limited resources''). There are statistically significant associations between greater redlining and pre-existing conditions like asthma, COPD, diabetes, hypertension, high cholesterol, kidney disease, obesity and stroke, all of which lead to a heightened risk of morbidity in COVID-19 patients. The report aims to understand how historical processes have created and maintained social, economic and environmental conditions that give rise to poor health. The study aims to place the historical in the present, the authors writing, ``The United States developed a deeply segregated society, the results of which are evident in the separation of communities we see today. Urban areas are often starkly divided by income and race. These divisions circumscribe the choices available to individuals.'' The divisions are also great determinants when it comes to the negative impact of COVID-19.},
  chapter = {Research},
  file = {/Users/alex/Zotero/storage/T2QQZ8Z8/holc-health.html},
  language = {en-US}
}

@article{ridgwayCaseSeriesFive2020,
