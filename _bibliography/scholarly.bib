@techreport{goffCenterPolicingEquity2020,
  title = {Center for {{Policing Equity COVID}}-19 {{Modeling Project}}},
  author = {Goff, Phillip Atiba and Haviland, Amelia M. and Lloyd, Tracey and Meyer, Mikaela and Warren, Rachel},
  year = {2020},
  month = oct,
  abstract = {A new model from the Center for Policing Equity finds that racial inequality in the United States has contributed to the spread of COVID-19, not only among communities of color, but broadly across all demographics. It has often been reported that Black and brown lives have been lost in greater percentages this year to coronavirus as people of color have been disproportionately represented in case counts. In essence, the model shows the impact of inequality on a nation as a whole, when it has been easier to compartmentalize the effects of COVID-19 in communities of color, often attributed to systemic racism. ``As case numbers and hospitalizations spike across the country,'' Goff and his colleagues and creators of the model note that ``our inability to protect ourselves and each other is directly tied to our inability to reckon with injustice.''  Goff and his coauthors continue, ``Racial disparities in our essential systems amplify the risk of infection for everyone, including those who may have imagined themselves as separate from the people behind the climbing casualty figures.'' Using the best data available to them, the authors simulated a city of five million to track COVID-19's spread over 40 days. The model allowed them to calculate how many people comprising the ``three essential systems with large racial disparities'' were infected and thus infected others: ``1) police officers and the people who they come into contact with, 2) people returning home from jail and prisons (known as `churn'), and 3) frontline, low-wage workers with no option but to keep showing up.'' The model showed that its three groups ``accounted for roughly 69 percent of new infections.''},
  file = {/Users/alex/Zotero/storage/DD2MGE2Z/Covid_Modeling_Project.pdf}
}

@techreport{gouldBlackWorkersFace2020,
  title = {Black Workers Face Two of the Most Lethal Preexisting Conditions for Coronavirus\textemdash Racism and Economic Inequality},
  author = {Gould, Elise and Wilson, Valerie},
  year = {2020},
  month = jun,
  pages = {1--30},
  institution = {{Economic Policy Institute}},
  abstract = {A new report by director of EPI's Program on Race, Ethnicity, and the Economy Valerie Wilson and senior economist Elise Gould explores how racial and economic inequality have left many black workers with few good options for protecting both their health and economic well-being during the coronavirus pandemic. Persistent racial disparities in health status, access to health care, wealth, employment, wages, housing, income, and poverty all contribute to greater susceptibility to the virus\textemdash both economically and physically. The authors explain there are three main groups of workers in the COVID-19 economy: those who have lost their jobs and face economic insecurity, those who are essential and face health insecurity, and those who are able to continue working from the safety of their homes. Black workers are least likely to be in that last group, with more economic and health security. As of April, less than half of the adult black population was employed. The black unemployment rate, even in the tightest of labor markets, remains significantly higher than the white unemployment rate. As of the latest data, the black unemployment rate is 16.7\%, compared with a white unemployment rate of 14.2\%. While black workers make up about one in nine workers overall, they make up about one in six of all front-line industry workers. They are disproportionately represented in employment in grocery, convenience, and drug stores, public transit, trucking, warehouse, and postal service, health care, and child care and social services. While, in the near-term, this protects them from job loss, it exposes them to greater likelihood of contracting COVID-19 while performing their jobs. Fewer than one in five black workers in the pre-pandemic economy were able to work from home.},
  file = {/Users/alex/Zotero/storage/IW4DZ5HL/Gould and Wilson - Black workers face two of the most lethal preexist.pdf},
  language = {en}
}

@article{goverAntiAsianHateCrime2020,
