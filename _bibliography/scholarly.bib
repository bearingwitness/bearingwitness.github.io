  keywords = {Access to information,African Americans,Community,Compliance,Coronaviruses,COVID-19,Disease control,Disease prevention,Disease transmission,Inequality,Medical Sciences,Minority \& ethnic groups,Pandemics,Politics,Polls \& surveys,Public health,Severe acute respiratory syndrome coronavirus 2,Shelter in place,Social distancing,Studies,United States--US},
