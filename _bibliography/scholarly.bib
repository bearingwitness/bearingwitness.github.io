}

@article{coughlinCOVID19AfricanAmericans2020,
  title = {{{COVID}}-19 {{Among African Americans}}: {{From Preliminary Epidemiological Surveillance Data}} to {{Public Health Action}}},
  author = {Coughlin, Steven S and Moore, Justin Xavier and George, Varghese and Johnson, J Aaron and Hobbs, Joseph},
