@article{azarDisparitiesOutcomesCOVID192020,
  title = {Disparities {{In Outcomes Among COVID}}-19 {{Patients In A Large Health Care System In California}}},
  author = {Azar, Kristen M. J. and Shen, Zijun and Romanelli, Robert J. and Lockhart, Stephen H. and Smits, Kelly and Robinson, Sarah and Brown, Stephanie and Pressman, Alice R.},
  year = {2020},
  month = may,
  pages = {10.1377/hlthaff.2020.00598},
  publisher = {{Health Affairs}},
  issn = {0278-2715},
  doi = {10.1377/hlthaff.2020.00598},
  abstract = {As the novel coronavirus disease (COVID-19) pandemic spreads throughout the United States, evidence is mounting that racial and ethnic minorities and socioeconomically disadvantaged groups are bearing a disproportionate burden of illness and death. We conducted a retrospective cohort analysis of COVID-19 patients at Sutter Health, a large integrated health system in northern California, to measure potential disparities. We used Sutter's integrated electronic health record to identify adults with suspected and confirmed COVID-19, and we used multivariable logistic regression to assess risk of hospitalization, adjusting for known risk factors, such as race/ethnicity, sex, age, health, and socioeconomic variables. We analyzed 1,052 confirmed cases of COVID-19 from the period January~1\textendash April~8, 2020. Among our findings, we observed that compared with non-Hispanic white patients, non-Hispanic African American patients had 2.7 times the odds of hospitalization, after adjustment for age, sex, comorbidities, and income. We explore possible explanations for this, including societal factors that either result in barriers to timely access to care or create circumstances in which patients view delaying care as the most sensible option. Our study provides real-world evidence of racial and ethnic disparities in the presentation of COVID-19.},
  file = {/Users/alex/Zotero/storage/3M25W8MH/Azar et al. - 2020 - Disparities In Outcomes Among COVID-19 Patients In.pdf;/Users/alex/Zotero/storage/948TPWS4/Azar et al. - 2020 - Disparities In Outcomes Among COVID-19 Patients In.pdf;/Users/alex/Zotero/storage/D3DNCCQ5/Azar et al. - 2020 - Disparities In Outcomes Among COVID-19 Patients In.pdf;/Users/alex/Zotero/storage/KPYMKZTE/Azar et al. - 2020 - Disparities In Outcomes Among COVID-19 Patients In.pdf;/Users/alex/Zotero/storage/NPAKDZ4N/Azar et al. - 2020 - Disparities In Outcomes Among COVID-19 Patients In.pdf;/Users/alex/Zotero/storage/AXGSYUX9/hlthaff.2020.html;/Users/alex/Zotero/storage/VHZJ87DF/hlthaff.2020.html},
  journal = {Health Affairs},
  keywords = {Adults,African Americans,California,Cohort analysis,Coronaviruses,COVID-19,Disadvantaged,Disadvantaged groups,Electronic health records,Electronic medical records,Ethnicity,Insurance,Minority \& ethnic groups,Mortality,Pandemics,Patients,Public Health And Safety,Regression analysis,Risk analysis,Risk factors,Sex,Social factors,Socio-economic aspects,United States--US,Viral diseases}
